<p id="notice"><%= notice %></p>

<table>
  <tr>
    <th>name</th>
    <th>states</th>
    <th>impressions</th>
    <th>clicks</th>
    <th>click rates</th>
  </tr>

  <tr>
    <td><%= link_to @keyword.name, @keyword %></td>
    <td>
      <ul>
          <% @keyword.states.each do |state| %>
            <li><%= state.name %></li>
          <% end -%>
      </ul>

    </td>

    <td><%= @keyword.impressionist_count %></td>

    <td><%= @keyword.impressions.where("message like '%click%'").count %></td>

    <td><%= @keyword.click_rate %> </td>
  </tr>
</table>

